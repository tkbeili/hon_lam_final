<a href="/all">Go Back</a>
<h1>Displaying Details for <%= @contact.name %></h1>
<p>ID: <%= @contact.id %></p>
<p>Email: <%= @contact.email %></p>
<p>Message: <%= @contact.message %></p>
<p>Note: <%= @contact.note %></p>

<form method="post" action="/contact/<%= @contact.id %>">
	<input type="hidden" name="_method" value="patch">
	<textarea id="note" name="note"><%= @contact.note %></textarea>
	<input type="submit" value="Save Note"></form>
</form>